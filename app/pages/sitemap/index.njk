{% extends "layout/default.njk" %}

{# Custom SEO tags, leave blank to apply the default values #}
{% set title = "サイトマップ｜EV Start Biz" %}
{% set description = "サイトマップを掲載しています。" %}
{% set keywords = "" %}

{# Page settings and also body class #}
{% set page = "sitemap" %}

{# Custom tags before </head> #}
{% block customHead %}
{% endblock %}

{# Custom tags before </body> #}
{% block customJs %}
{% endblock %}

{# Main content #}
{% block content %}

<!-- breadcrumbs -->
<div class="breadcrumb">
  <div class="containers--sm">
    <ul class="flex fS">
      <li>
        <a href="/">ホーム</a>
      </li>
      <li>
        サイトマップ
      </li>
    </ul>
  </div>
</div>

<main class="sitemap__main">

  <!-- company-head -->
  <div class="sitemap__head sitemap-head page-head --none">
    <div class="sitemap-head__containers containers--md">
      <div class="sitemap-head__ttl ttl-primary --center">
        <div class="img">
          <img src="/assets/img/common/ttl-sitemap.png" alt="Site Map">
        </div>
        <h1 class="ttl">サイトマップ</h1>
      </div>
    </div>
  </div>

  <!-- operating-company__sec -->
  <section class="sitemap__sec">
    <div class="containers--sm">
      <div class="sitemap__items" data-aos="fade-up">
        <nav class="sitemap__nav sitemap-nav">
          <div class="sitemap-nav__content">
            <p class="parent">
              <a href="/about/">『EV Start Biz』 とは</a>
            </p>
            <p class="parent">
              <a href="/ev_introduction/">EV車導入をお考えの企業さまへ</a>
            </p>
            <p class="parent">
              <a href="/qa/">よくあるギモン</a>
            </p>
            <p class="parent">
              <a href="/solution/enneev/">ソリューション</a>
            </p>
            <ul class="sitemap-nav__list">
              <li class="sitemap-nav__item">
                <a href="">&nbsp;スマート充電サービス EnneEV®</a>
              </li>
            </ul>
          </div>
          <div class="sitemap-nav__content">
            <p class="parent">
              <a href="">お役立ち情報</a>
            </p>
            <ul class="sitemap-nav__list">
              <li class="sitemap-nav__item">
                <a href="/column/">&nbsp;お役立ちコラム</a>
              </li>
              <li class="sitemap-nav__item">
                <a href="/useful/">&nbsp;お役立ち資料</a>
              </li>
              <li class="sitemap-nav__item">
                <a href="/news/">&nbsp;ニュースリリース</a>
              </li>
            </ul>
            <p class="parent">
              <a href="">お問合せ</a>
            </p>
            <ul class="sitemap-nav__list">
              <li class="sitemap-nav__item">
                <a href="">&nbsp;サービス資料</a>
              </li>
              <li class="sitemap-nav__item">
                <a href="">&nbsp;ご相談・お問合せ</a>
              </li>
            </ul>
          </div>
          <div class="sitemap-nav__content">
            <p class="parent">
              <a href="#">プライバシーポリシー</a>
            </p>
            <p class="parent">
              <a href="">Cookieポリシー</a>
            </p>
            <p class="parent">
              <a href="/operating_company/">運営会社</a>
            </p>
          </div>
        </nav>

      </div>
    </div>
  </section>

  <!-- contact -->
  <section class="useful__contact common-contact">
    <div class="containers--sm">
      <div class="common-contact__ttl ttl-primary --center">
        <div class="img">
          <img src="/assets/img/common/ttl-contact.png" alt="contact us">
        </div>
        <h2 class="ttl">ご相談・お問合せ</h2>
      </div>
      <p class="common-contact__text" data-aos="fade-up">EV導入や運用に関する<span>お問合せ・ご相談はお気軽に</span><span>ご連絡ください。</span></p>
      <div class="common-contact__btn" data-aos="fade-up">
        <a href="/contact/" class="btn02">まずは相談・問合せする</a>
      </div>
      <div class="common-contact__document" data-aos="fade-up">
        <p class="common-contact__document-text">サービス資料・お役立ち資料を<span>ご希望の方はこちら</span></p>
        <div class="common-contact__document-btn">
          <a href="#" class="btn02 --yellow --sp-small">サービス資料</a>
          <a href="#" class="btn02 --yellow --sp-small">お役立ち資料</a>
        </div>
      </div>
    </div>
  </section>

</main>


{% endblock %}