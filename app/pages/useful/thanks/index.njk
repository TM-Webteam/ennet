{% extends "layout/default.njk" %}

{# Custom SEO tags, leave blank to apply the default values #}
{% set title = "お役立ち資料ダウンロードサンクスページ｜EV Start Biz" %}
{% set description = "資料をダウンロードいただきありがとうございました。" %}
{% set keywords = "" %}

{# Page settings and also body class #}
{% set page = "utility-thanks" %}

{# Custom tags before </head> #}
{% block customHead %}
{% endblock %}

{# Custom tags before </body> #}
{% block customJs %}
{% endblock %}

{# Main content #}
{% block content %}

<!-- breadcrumbs -->
<div class="breadcrumb">
  <div class="containers--sm">
    <ul class="flex fS">
      <li>
        <a href="#">ホーム</a>
      </li>
      <li>
        お役立ち資料
      </li>
    </ul>
  </div>
</div>

<main class="utility-thanks__main">

  <!-- thanks -->
  <section class="utility-thanks__sec thanks">
    <div class="containers">
      <h1 class="thanks__ttl">
        資料をダウンロードいただき<br class="sp-only">ありがとうございました。
      </h1>
      <p class="thanks__lead">
        ご入力いただきましたメールアドレス宛に<br>
        資料のダウンロード用URLを送信させていただきました。
      </p>
      <div class="thanks__back">
        <a href="/" class="btn01 --large">トップページに戻る</a>
      </div>
    </div>
  </section>

  <!-- thanks__sec1 -->
  <section class="thanks__contact common-contact">
    <div class="containers--sm">
      <div class="common-contact__ttl ttl-primary --center">
        <div class="img">
          <img src="/assets/img/common/ttl-contact.png" alt="contact us">
        </div>
        <h2 class="ttl">ご相談・お問合せ</h2>
      </div>
      <p class="common-contact__text" data-aos="fade-up">EV導入や運用に関する<span>お問合せ・ご相談はお気軽に</span><span>ご連絡ください。</span></p>
      <div class="common-contact__btn" data-aos="fade-up">
        <a href="#" class="btn02">まずは相談・問合せする</a>
      </div>
      <div class="common-contact__document" data-aos="fade-up">
        <p class="common-contact__document-text">サービス資料・お役立ち資料を<span>ご希望の方はこちら</span></p>
        <div class="common-contact__document-btn">
          <a href="#" class="btn02 --yellow --sp-small">サービス資料</a>
          <a href="#" class="btn02 --yellow --sp-small">お役立ち資料</a>
        </div>
      </div>
    </div>
  </section>

</main>


{% endblock %}