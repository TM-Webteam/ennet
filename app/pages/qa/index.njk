{% extends "layout/default.njk" %}

{# Custom SEO tags, leave blank to apply the default values #}
{% set title = "EV導入をお考えの企業さまへ｜EV Start Biz" %}
{% set description = "EV導入までの流れや、導入検討中の企業さまへおすすめのコンテンツについてご紹介します。" %}
{% set keywords = "" %}

{# Page settings and also body class #}
{% set page = "qa" %}

{# Custom tags before </head> #}
{% block customHead %}
{% endblock %}

{# Custom tags before </body> #}
{% block customJs %}
{% endblock %}

{# Main content #}
